{
	"format_version": "1.13.0",
	"minecraft:feature_rules": {
		"description": {
			"identifier": "minecraft:bamboo_jungle_after_surface_tall_grass_feature_rules",
			"places_feature": "kado:overworld_block_filter"
		},
		"conditions": {
			"placement_pass": "after_surface_pass",
			"minecraft:biome_filter": [
				{ "test": "has_biome_tag", "value": "bamboo" },
				{ "test": "has_biome_tag", "value": "jungle" }
			]
		},
		"distribution": {
			"iterations": 1,
			"scatter_chance": "v.is_legacy ? 0.0 : 100.0",
			"x": {
				"distribution": "uniform",
				"extent": [0, 16]
			},
			"y": "q.heightmap(v.worldx, v.worldz)",
			"z": {
				"distribution": "uniform",
				"extent": [0, 16]
			}
		}
	}
}
